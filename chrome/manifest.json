{
  "name": "Airflow Extension",
  "version": "1.1.0",
  "description": "Automatically generate commands from airflow UI",
  "permissions": ["activeTab", "declarativeContent"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "128": "images/logo.png"
    }
  },
  "icons": {
    "128": "images/logo.png"
  },
  "manifest_version": 2
}
